[color]
	ui     = auto
  	diff   = auto
  	status = auto
  	branch = auto
[alias]
  	co = checkout
[push]
  	default = current
[format]
  	pretty = %C(yellow)%h%Creset %s %C(red)(%cr)%Creset
[core]
  	excludesfile = ~/.gitignore
  	editor       = vim
[branch]
  	autosetuprebase = always
[include]
  	path = ~/.gitconfig.local

